<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsStudent" targetNamespace="http://www.tempuri.org/dsStudent.xsd" xmlns:mstns="http://www.tempuri.org/dsStudent.xsd" xmlns="http://www.tempuri.org/dsStudent.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsStudent" msdata:IsDataSet="true" msdata:Locale="zh-CN">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="student">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="stu_id" type="xs:string" />
              <xs:element name="name" type="xs:string" minOccurs="0" />
              <xs:element name="sex" type="xs:string" minOccurs="0" />
              <xs:element name="addr" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:student" />
      <xs:field xpath="mstns:stu_id" />
    </xs:unique>
  </xs:element>
</xs:schema>